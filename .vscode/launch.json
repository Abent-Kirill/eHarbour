{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Extension",
			"type": "node",
			"request": "launch",
			"runtimeExecutable": "code", // Замените на путь к вашему VS Code, если нужно
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}",
				"--disable-extensions"
			],
			"outFiles": ["${workspaceFolder}/out/**/*.js"]
		}
	]
}
